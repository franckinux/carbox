pkgver=master
pkgrel=1
pkgdesc=""
arch=('any')
license=('BSD')
url="https://pypi.python.org/pypi/$pkgname/"
makedepends=('python' 'python-setuptools')
depends=('python' )
options=(!emptydirs)
source=(https://github.com/franckinux/aiompd/archive/master.zip)
# source=(https://github.com/zzzsochi/aiompd/archive/$pkgver.tar.gz)
md5sums=('e0a65cdee96fb960acad2f201826b99a')
